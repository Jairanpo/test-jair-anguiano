{
  "name": "test-jair-anguiano",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jair Anguiano",
  "license": "MIT",
  "scripts": {
    "build:server": "babel-node src/server/expressServer.js",
    "build:client": "webpack -config=config/webpack.config.client.js",
    "start:server": "nodemon --exec babel-node src/server/expressServer.js",
    "start:client": "webpack --watch --config=config/webpack.config.client.js",
    "dev": "npm run start:client & npm run start:server",
    "start:mac": "npm run build:client & npm run build:server",
    "dev:win": "concurrently \"npm run start:client\" \"npm run start:server\""
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "nodemon": "^2.0.3",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  }
}